<h1>Admin Sign In</h1>
<% if flash[:error] %>
    <p><%= flash[:error] %></p>
<% end %>

<%= form_for(:session, :url =>adminsessions_path) do |f| %>
    <div class="field">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>



    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>